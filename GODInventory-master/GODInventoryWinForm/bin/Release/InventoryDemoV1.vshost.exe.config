  <userSettings>
    <GODInventoryWinForm.Properties.Settings>
      <setting name="NFWEInstallDir" serializeAs="String">
        <value>d:\NAFCO\NFWE\client</value>
      </setting>
    </GODInventoryWinForm.Properties.Settings>
  </userSettings>
  <system.data>
    <DbProviderFactories>
